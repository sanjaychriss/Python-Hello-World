pipeline {
  agent any
  stages {
    stage('Buzz Buzz') {
      steps {
        echo 'Buzz! Buzz!'
      }
    }

    stage('Bees Bees') {
      steps {
        echo '"Buzz, Bees, Buzz!"'
        echo '"Bees Buzzing!"'
      }
    }

    stage('3 Bees') {
      steps {
        echo '"3 bees"'
      }
    }

  }
}